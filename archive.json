      "id": "I_kwDOH7Dq_M5SLLBB",
      "title": "Directorate Members in Path 2",
      "url": "https://github.com/ietf-wg-elegy/rfc8989bis/issues/1",
      "state": "CLOSED",
      "author": "martinduke",
      "authorAssociation": "COLLABORATOR",
